  <%- include('./includes/head.ejs') %>
</head>
<body>
  <%- include('./includes/nav.ejs') %>
  <div class="container" style="margin-top: 30px;">
    <% if(userObj.vehicleRegistrations.length == 0) { %>
      <div class="row d-flex justify-content-center">
        <div class="card text-center col-8">
          <div class="card-body">
            <p class="card-text">You have no Vehicle Registered</p>
          </div>
        </div>
      </div>
    <% }  else { %>
      <% for(let vehicle of userObj.vehicleRegistrations) { %>
        <div class="row d-flex justify-content-center">
          <div class="card text-center col-8">
            <div class="card-body">
              <p class="card-title">Vehicle ID: <%= vehicle.vehicle.id %></p>
              <p class="card-text">Model Name: <%= vehicle.vehicle.name %></p>
              <p class="card-text">Vehicle Type: <%= vehicle.vehicle.type %></p>
            </div>
          </div>
        </div>
      <% } %>  
    <% } %>
  </div>
  <%- include('./includes/tail.ejs') %>